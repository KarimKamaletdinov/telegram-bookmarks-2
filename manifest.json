{
  "manifest_version": 2,
  "name": "Telegram Bookmarks2",
  "version": "1.0",
  "description": "Sends link to added bookmark in Telegram bot",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/bookmark-it",
  "icons": {
    "48": "icons/tb.png"
  },
    "permissions": [
      "bookmarks",
      "tabs"
    ],
    "background": {
      "scripts": [
        "src/tb.js"
      ]
    },
    "options_ui": {
      "page": "src/options.html"
    }
  }